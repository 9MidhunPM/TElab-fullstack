# ================================
# TElab Fullstack Application
# Environment Variables Template
# ================================
# Copy this file to .env and fill in your actual values
# Never commit .env files with real credentials!

# ================================
# Backend Configuration (Spring Boot)
# ================================

# JWT Configuration
JWT_SECRET=myJwtSecretKeyThatIsLongEnoughForHMACAlgorithmMinimum256Bits
JWT_EXPIRATION=86400000

# CORS Configuration
CORS_ALLOWED_ORIGINS=*

# Server Configuration
PORT=8080

# External API Configuration
ETLAB_API_BASE_URL=https://etlabapi.onrender.com/api

# ================================
# Frontend Configuration (Expo/React Native)
# ================================

# API Base URL - Point this to your backend
EXPO_PUBLIC_API_BASE_URL=https://your-backend-url.com

# Add other frontend environment variables as needed
# EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id
# EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
